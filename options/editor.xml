<application>
  <component name="CodeVisionSettings">
    <option name="defaultPosition" value="Right" />
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="DeclarativeInlayHintsSettings">
    <option name="enabledOptions">
      <map>
        <entry key="kotlin.lambdas.hints#hints.lambda.receivers.parameters" value="true" />
      </map>
    </option>
    <option name="providerIdToEnabled">
      <map>
        <entry key="kotlin.lambdas.hints" value="true" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <settingsMapElement>
      <kotlin.kotlin.references.types.hints>
        <Settings>
          <option name="propertyType" value="true" />
        </Settings>
      </kotlin.kotlin.references.types.hints>
      <kotlin.kotlin.lambdas.hints>
        <Settings>
          <option name="implicitReceiversAndParams" value="false" />
          <option name="returnExpressions" value="false" />
        </Settings>
      </kotlin.kotlin.lambdas.hints>
    </settingsMapElement>
  </component>
  <component name="InlineCompletionOnboarding">
    <option name="onboardingFinished" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>